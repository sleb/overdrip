{
  "name": "overdrip",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "devDependencies": {
    "@types/bun": "1.3.2",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1"
  },
  "scripts": {
    "build": "bun run --workspaces --if-present build",
    "clean": "bun run --workspaces --if-present clean",
    "dev": "bun run --workspaces --if-present dev",
    "lint": "bun run --workspaces lint",
    "typecheck": "bun run --workspaces typecheck"
  }
}