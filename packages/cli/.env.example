# Example environment configuration for Overdrip CLI
# Copy this file to .env and fill in your values

# Google OAuth 2.0 Client ID
# Get this from Google Cloud Console > APIs & Services > Credentials
# Application type should be "Desktop application"
GOOGLE_OAUTH_CLIENT_ID=your_google_client_id.googleusercontent.com

# Optional: Use Firebase emulators for development
# USE_EMULATOR=true
